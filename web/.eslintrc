{
  "extends": ["arun", "arun/typescript", "arun/react", "plugin:@next/next/recommended", "prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["import"],
  "rules": {
    "import/no-extraneous-dependencies": "off", // not for monorepo
    "no-useless-escape": "off"
  }
}
