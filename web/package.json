{
  "name": "@dimension/web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n tailwind,next 'pnpm run tailwind:watch' 'next dev -p 3000'",
    "start": "next start",
    "build": "next build",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test": "run-s test:*",
    "storybook": "concurrently -n tailwind,storybook -c cyan,magentaBright 'pnpm run tailwind:watch' 'start-storybook -p 6006'",
    "storybook:build": "build-storybook",
    "tailwind:build": "npx tailwindcss -i ./src/styles/tailwind-input.css -o ./src/styles/tailwind-output.css",
    "tailwind:watch": "npx tailwindcss -i ./src/styles/tailwind-input.css -o ./src/styles/tailwind-output.css --watch",
    "clean": "rm -rf .turbo node_modules"
  },
  "dependencies": {
    "@dimension/api": "workspace:*",
    "@dimension/auth": "workspace:*",
    "@dimension/db": "workspace:*",
    "@radix-ui/react-avatar": "^1.0.1",
    "@radix-ui/react-separator": "^1.0.1",
    "@radix-ui/react-switch": "^1.0.1",
    "@radix-ui/react-toggle-group": "^1.0.2",
    "@tanstack/react-query": "^4.24.9",
    "@trpc/client": "^10.12.0",
    "@trpc/next": "^10.12.0",
    "@trpc/react-query": "^10.12.0",
    "@trpc/server": "^10.12.0",
    "clsx": "^1.1.1",
    "experimental-prisma-webpack-plugin": "^0.0.5",
    "next": "^13.2.0",
    "next-auth": "^4.20.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@next/eslint-plugin-next": "^13.1.6",
    "@playwright/test": "^1.31.1",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@types/react-beautiful-dnd": "^13.1.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint-config-next": "^13.1.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.9.0",
    "typescript": "^4.9.5",
    "vite": "^3.1.3",
    "vitest": "^0.29.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
